<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="wipay_form">
            <div>
                <input type="hidden" name="data_set" t-att-data-action-url="tx_url" data-remove-me=""/>
                <input type="hidden" name="total" t-att-value="total"/>
                <input type="hidden" name="email" t-att-value="email"/>
                <input type="hidden" name="name" t-att-value="name"/>
                <input type="hidden" name="phone" t-att-value="phone"/>
                <input type="hidden" name="order_id" t-att-value="order_id"/>
                <input type="hidden" name="environment" t-att-value="environment"/>
                <input type="hidden" name="currency" t-att-value="currency"/>
                <input type="hidden" name="country_code" t-att-value="country_code"/>
                <input type="hidden" name="fee_structure" t-att-value="fee_structure"/>
                <input type="hidden" name="method" t-att-value="method"/>
                <input type="hidden" name="origin" t-att-value="origin"/>
                <input type="hidden" name="response_url" t-att-value="response_url" />
                <input type="hidden" name="account_number"  t-att-value='account_number'/>

            </div>
        </template>
    </data>
</odoo>
